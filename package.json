{
  "description": "A CSS implementation of the Stellar design system.",
  "devDependencies": {
    "@types/eslint": "^8.56.7",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.1.1",
    "stylelint": "^14.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^5.3.2",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0-alpha.20",
    "vite": "^5.0.11"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.8.1"
  },
  "exports": {
    "css": "./packages/css/dist/css/index.css",
    "css/*.*": "./packages/css/dist/css/*.*",
    "react/*.*": "./packages/react/dist/*.*",
    "svelte/*.*": "./packages/svelte/dist/*.*"
  },
  "files": [
    "/packages/css/dist",
    "/packages/react/dist",
    "/packages/svelte/dist",
    "!/packages/svelte/dist/**/*.spec.*",
    "!/packages/svelte/dist/**/*.test.*"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa-jpl/stellar.git"
  },
  "name": "@nasa-jpl/stellar",
  "scripts": {
    "build:css": "cd ./packages/css && npm run build",
    "build:svelte": "cd ./packages/svelte && npm run build",
    "build:react": "cd ./packages/react && npm run build",
    "build": "npm run build:css && npm run build:react && npm run build:svelte"
  },
  "type": "module",
  "version": "1.1.18",
  "workspaces": {
    "packages": [
      "packages/css",
      "packages/react",
      "packages/svelte"
    ]
  }
}
